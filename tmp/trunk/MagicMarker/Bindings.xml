<Bindings>
	<Binding name="MAGICMARKTOGGLE" header="MagicMarker">
		MagicMarker:ToggleConfigDialog()
	</Binding>
	<Binding name="MAGICMARKRESET" header="MagicMarker">
		MagicMarker:ResetMarkData()
	</Binding>
	<Binding name="MAGICMARKMARK" header="MagicMarker">
		MagicMarker:MarkSingle()
	</Binding>
	<Binding name="MAGICMARKUNMARK" header="MagicMarker">
		MagicMarker:UnmarkSingle()
	</Binding>
	<Binding name="MAGICMARKRAID" header="MagicMarker">
		MagicMarker:MarkRaidTargets()
	</Binding>
	<Binding name="MAGICMARKLOAD" header="MagicMarker">
                MagicMarker:MarkRaidFromCache()
	</Binding>
	<Binding name="MAGICMARKSAVE" header="MagicMarker">
		MagicMarker:CacheRaidMarks()
	</Binding>
        <Binding name="MAGICMARKSMARTMARK" header="MagicMarker" runOnUp="true">
                MagicMarker.markKeyDown = keystate == "down"
        </Binding>
</Bindings>
